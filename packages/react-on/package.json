{
  "name": "react-on",
  "description": "Simple React instrumentation library for tracking user interactions",
  "type": "module",
  "homepage": "https://gkurt.com/react-on/",
  "repository": {
    "type": "git",
    "url": "http://github.com/KurtGokhan/react-on"
  },
  "keywords": ["react", "tracking", "analytics", "instrumentation"],
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Gokhan Kurt",
    "email": "krtgkn@gmail.com",
    "url": "https://github.com/KurtGokhan"
  },
  "files": ["README.md", "dist"],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./middlewares": {
      "require": "./dist/middlewares/index.cjs",
      "default": "./dist/middlewares/index.js"
    }
  },
  "scripts": {
    "vite": "vite",
    "vitest": "vitest",
    "start": "vite build --watch",
    "build": "vite build",
    "clean": "rimraf dist",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "18.2.69",
    "@types/react-dom": "^18",
    "jsdom": "^24.0.0",
    "jsx-middlewares": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "5.4.3",
    "vite": "^5.2.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "jsx-middlewares": ">=1",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "jsx-middlewares": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
