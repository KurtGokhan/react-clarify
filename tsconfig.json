{
  "compilerOptions": {
    "composite": true,
    "outDir": "out-tsc",
    "paths": {},
    "noEmit": false,
    "declaration": true,
    "target": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM"]
  }
}
