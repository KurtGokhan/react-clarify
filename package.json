{
  "name": "react-on",
  "description": "Simple React instrumentation library for tracking user interactions",
  "homepage": "https://gkurt.com/react-on/",
  "repository": {
    "type": "git",
    "url": "http://github.com/KurtGokhan/react-on"
  },
  "keywords": [
    "react",
    "tracking",
    "analytics",
    "instrumentation"
  ],
  "version": "1.0.1",
  "license": "MIT",
  "author": {
    "name": "Gokhan Kurt",
    "email": "krtgkn@gmail.com",
    "url": "https://github.com/KurtGokhan"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./middlewares": "./dist/middlewares/index.js"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.lib.json",
    "start": "yarn tsc --watch --incremental",
    "build": "yarn clean && yarn tsc",
    "clean": "rimraf dist && rimraf tsconfig.lib.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "yarn build"
  },
  "dependencies": {
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/react": "18.2.47",
    "@types/react-dom": "^18",
    "@types/react-is": "^18",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.1.0",
    "jsx-middlewares": "^1.0.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "jsx-middlewares": ">=1",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "jsx-middlewares": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "4.0.2"
  }
}
